{
  "moduleFileExtensions": [
    "js",
    "json",
    "coffee",
    "litcoffee",
    "coffee.md"
  ],
  "transform": {
    "coffee|litcoffee|coffee.md": "jest-preset-coffeescript"
  },
  "testRegex": "tests?/.*\\.(coffee|litcoffee|coffee.md)$",
  "testPathIgnorePatterns": [
    "node_modules",
    "fixtures"
  ]
}
